@model Dictionary<string,string> 
<!-- Subscribe Start -->
<div class="subscribe-area pt-60 pb-70">
    <div class="container">
        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                <div class="subscribe-content section-title text-center">
                    <h2>subscribe our newsletter</h2>
                    <p>I must explain to you how all this mistaken idea </p>
                </div>
                <div class="newsletter-form mc_embed_signup">
                    <form asp-controller="subscribe" asp-action="create" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
                        <div id="mc_embed_signup_scroll" class="mc-form">
                            <input type="email" value="" name="EMAIL" class="email" id="mce-EMAIL" placeholder="Enter your e-mail address" required>
                            <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
                            <div class="mc-news" aria-hidden="true"><input type="text" name="b_6bbb9b6f5827bd842d9640c82_05d85f18ef" tabindex="-1" value=""></div>
                            <button id="mc-embedded-subscribe" class="default-btn" type="submit" name="subscribe"><span>subscribe</span></button>
                        </div>
                    </form>
                    <!-- mailchimp-alerts Start -->
                    <div class="mailchimp-alerts">
                        <div class="mailchimp-submitting"></div><!-- mailchimp-submitting end -->
                        <div class="mailchimp-success"></div><!-- mailchimp-success end -->
                        <div class="mailchimp-error"></div><!-- mailchimp-error end -->
                    </div>
                    <!-- mailchimp-alerts end -->
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Subscribe End -->
<footer class="footer-area">
    <div class="main-footer">
        <div class="container">
            <div class="row">
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <div class="single-widget pr-60">
                        <div class="footer-logo pb-25">
                            <a asp-controller="home" asp-action="index"><img src="~/img/logo/@Model.FirstOrDefault(d=>d.Key=="FooterLogo").Value" alt="eduhome"></a>
                        </div>
                        <p>@Model.FirstOrDefault(d=>d.Key=="Description").Value </p>
                        <div class="footer-social">
                            <ul>
                                <li><a href="@Model.FirstOrDefault(d=>d.Key=="Facebook").Value"><i class="zmdi zmdi-facebook"></i></a></li>
                                <li><a href="@Model.FirstOrDefault(d=>d.Key=="Pinterest").Value"><i class="zmdi zmdi-pinterest"></i></a></li>
                                <li><a href="@Model.FirstOrDefault(d=>d.Key=="Linkedin").Value"><i class="zmdi zmdi-linkedin"></i></a></li>
                                <li><a href="@Model.FirstOrDefault(d=>d.Key=="Twitter").Value"><i class="zmdi zmdi-twitter"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 col-sm-6 col-xs-12">
                    <div class="single-widget">
                        <h3>information</h3>
                        <ul>
                            <li><a asp-controller="home" asp-action="@Model.FirstOrDefault(d=>d.Key=="About").Value">About</a></li>
                            <li><a asp-controller="home" asp-action="@Model.FirstOrDefault(d=>d.Key=="Blog").Value">Blog</a></li>
                            <li><a asp-controller="home" asp-action="@Model.FirstOrDefault(d=>d.Key=="Event").Value">Event </a></li>
                            <li><a asp-controller="home" asp-action="@Model.FirstOrDefault(d=>d.Key=="Courses").Value">Courses</a></li>
                            <li><a asp-controller="home" asp-action="@Model.FirstOrDefault(d=>d.Key=="Teacher").Value">Teacher</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-2 col-sm-6 col-xs-12">
                    <div class="single-widget">
                        <h3>useful links</h3>
                        <ul>
                            <li><a asp-controller="home" asp-action="@Model.FirstOrDefault(d=>d.Key=="About").Value">About</a></li>
                            <li><a asp-controller="home" asp-action="@Model.FirstOrDefault(d=>d.Key=="Blog").Value">Blog</a></li>
                            <li><a asp-controller="home" asp-action="@Model.FirstOrDefault(d=>d.Key=="Event").Value">Event </a></li>
                            <li><a asp-controller="home" asp-action="@Model.FirstOrDefault(d=>d.Key=="Courses").Value">Courses</a></li>
                            <li><a asp-controller="home" asp-action="@Model.FirstOrDefault(d=>d.Key=="Teacher").Value">Teacher</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-3 col-sm-6 col-xs-12">
                    <div class="single-widget">
                        <h3>get in touch</h3>
                        <p>Your address goes here, Street<br>@Model.FirstOrDefault(d=>d.Key=="Adress").Value</p>
                        <p>@Model.FirstOrDefault(d=>d.Key=="Phone1").Value<br>@Model.FirstOrDefault(d=>d.Key=="Phone2").Value</p>
                        <p><a href="@Model.FirstOrDefault(d=>d.Key=="URL").Value">@Model.FirstOrDefault(d=>d.Key=="URL").Value</a><br><a href="@Model.FirstOrDefault(d=>d.Key=="URL").Value">@Model.FirstOrDefault(d=>d.Key=="URL").Value</a></p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="footer-bottom text-center">
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <p>Copyright © <a href="#" target="_blank">HasTech</a> 2017. All Right Reserved By Hastech.</p>
                </div>
            </div>
        </div>
    </div>
</footer>
