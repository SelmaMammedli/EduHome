@model Dictionary<string,string>
<header class="top">
    <div class="header-top">
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-sm-8 col-xs-12">
                    <div class="header-top-left">
                        <p>HAVE ANY QUESTION ?  @Model.FirstOrDefault(d=>d.Key=="Phone1").Value</p>
                    </div>
                </div>
                <div class="col-md-4 col-sm-4 col-xs-12">
                    <div class="header-top-right text-right">
                        <ul>
                            @if (User.Identity.IsAuthenticated)
                            {

                                <li> <a>@User.Identity.Name</a></li>

                                <li><a asp-controller="account" asp-action="logout">Logout</a></li>
                            }
                            else
                            {

                                <li><a asp-controller="account" asp-action="login">Login</a></li>
                            }
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="header-area header-sticky fixed">
        <div class="container">
            <div class="row">
                <div class="col-md-3 col-sm-3 col-xs-6">
                    <div class="logo">
                        <a asp-controller="home" asp-action="index"><img src="~/img/logo/@Model.FirstOrDefault(d=>d.Key=="LogoImageUrl").Value" alt="eduhome" /></a>
                    </div>
                </div>
                <div class="col-md-9 col-sm-9 col-xs-12">
                    <div class="content-wrapper one">
                        <!-- Main Menu Start -->
                        <div class="main-menu one text-right">
                            <nav>
                                <ul>
                                    <li>
                                        <a asp-action="Index">Home</a>

                                    </li>
                                    <li><a asp-action="About">About</a></li>
                                    <li>
                                        <a asp-action="Courses">courses</a>

                                    </li>
                                    <li>
                                        <a asp-action="Event">event</a>

                                    </li>
                                    <li class="hidden-sm">
                                        <a asp-action="Teacher">teachers</a>

                                    </li>
                                    <li>
                                        <a asp-action="Blog">blog</a>

                                    </li>
                                    <li><a asp-action="Contact">Contact</a></li>
                                    <li><a href="#">Buy Now</a>
                                </ul>
                            </nav>
                        </div>
                        <div class="mobile-menu hidden-lg hidden-md one"></div>
                        <!-- Main Menu End -->
                    </div>
                </div>
            </div>
        </div>
    </div>
</header>
