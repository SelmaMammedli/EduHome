@model IEnumerable<Courses>


@foreach(var course in Model)
{
    <li>
        <a asp-controller="home" asp-action="CoursesDetails" asp-route-id="@course.Id">
          <img style="border-radius:50%" width="50px" height="50px" src="~/img/course/@course.ImageUrl">  @course.Category.Title @course.Price
        </a>
    </li>

}